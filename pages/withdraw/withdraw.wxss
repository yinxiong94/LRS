  border-radius: 20rpx;
  text-align: center;
  line-height: 58rpx;
  margin-right: 50rpx;
  margin-top: 30rpx;
}

.box:nth-child(3n) {
  margin-right: 0;
}

.t5 {
  font-size: 36rpx;
