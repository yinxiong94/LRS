.sbox view {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 54rpx;
  margin-left: 16rpx;
}

radio {
  transform: scale(0.8);
}

radio .wx-checkbox-input {
  border-radius: 28rpx;
  height: 56rpx;
  width: 56rpx;
  margin-top: -4rpx;
}

radio .wx-radio-input.wx-radio-input-checked {
  background: #fbc00e;
  border-color: #fbc00e;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  width: 40rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 38rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.shuru {
  border-top: 1rpx solid #eee;
  display: flex;
  line-height: 90rpx;
}

.shuru input {
  height: 90rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.shuru view {
  width: 25%;
}

.box-shuru {
  width: 750rpx;
  height: 390rpx;
  background: rgba(245, 245, 245, 1);
  padding: 60rpx 30rpx;
  box-sizing: border-box;
}

.ssbox {
