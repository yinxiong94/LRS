page {
  background-color: #f5f5f5;
}

.compile {
  height: 70rpx;
  line-height: 70rpx;
  margin-left: 20rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.compile_list {
  background-color: #fff;
  padding: 0 20rpx;
}

.portrait {
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.portrait_height {
  height: 131rpx;
}

.portrait_image {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 250rpx;
}

.portrait_image image {
  width: 99rpx;
  height: 100rpx;
  border-radius: 50%;
}

.bracket {
  color: rgba(185, 185, 185, 1);
}

.portrait_height1 {
  height: 101rpx;
}

.name {
  display: flex;
  text-align: center;
  justify-content: space-between;
  width: 180rpx;
}

.name>text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.portrait_height1>text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.btn {
  width: 690rpx;
  height: 80rpx;
  background: rgba(254, 213, 23, 1);
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
  margin: 50rpx auto;
}
