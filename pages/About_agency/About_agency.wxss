.location{
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.location_name{
  color:rgba(225,147,46,1);
  font-size:30rpx;
  display: flex;
  align-items: center;
}

.location_name image{
  width: 34rpx;
  height: 44rpx;
  margin-right: 11rpx;
}

.location_jia{
  display: flex;
  align-items: center;
}

.location_jia image:nth-child(1){
    width: 35rpx;
    height: 36rpx;
}

.location_jia image:nth-child(2){
    width: 41rpx;
    height: 41rpx;
    margin-left: 12rpx;
}

.swiper{
  width: 100%;
  margin-top: 35rpx;
}

.swiper1{
  height: 300rpx;
}

.swiper-item{
  width: 100%;
  height: 300rpx !important;
}

.swiper-item image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.agency{
    font-size:30rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(0,0,0,1);
    margin-top: 54rpx;
    display: flex;
    width: 70%;
    justify-content: space-between;
    align-items: center;
}

.agency_weight{
  font-size: 36rpx;
  font-weight: bold;
}

.game {
  width: 691rpx;
  height: 233rpx;
  background: rgba(255, 255, 255, 1);
  /* box-shadow: 0px 5rpx 12rpx 1rpx rgba(166, 166, 166, 1); */
  border-radius: 26rpx;
  margin: 0 auto;
  margin-top: 10rpx;
  padding: 20rpx 10rpx;
  display: flex;
}
.game1_list{
  height: 160px;
  box-shadow: 0px 5px 12px 1px rgba(241, 246, 255, 1);
  /* border: 1px solid red; */
  margin-top: 40rpx;
  padding: 20rpx;
  position: relative;
}
.game2{
   width: 691rpx;
  height: 176rpx;
  background: rgba(255, 255, 255, 1);
  /* box-shadow: 0px 5rpx 12rpx 1rpx RGBA(241, 246, 255, 1); */
  border-radius: 26rpx;
  margin: 0 auto;
  margin-top: 10rpx;
  /* padding: 20rpx 10rpx; */
  display: flex;
}
.game_Img {
  width: 200rpx;
  height: 100%;
  margin-right: 10rpx;
}

.game_Img image {
  width: 100%;
  height: 100%;
}

.game_title_img image {
  width: 28rpx;
  height: 27rpx;
}

.game1 {
  width: 520rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.game11{
  width: 520rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.game_title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.game_title1 text{
  /* margin: 0 10rpx; */
  font-size: 32rpx;
  font-weight:bold;
}
.game_title>text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(244, 142, 10, 1);
}

.game1_title_img {
  display: flex;
  align-items: center;
}

.game_title_img>text {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(155, 155, 155, 1);
  display: inline-block;
  margin-left: 10rpx;
  width: 182rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.details {
  display: flex;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  width: 100%;
  justify-content: space-between;
}
.details1{
  display: flex;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  width: 60%;
  justify-content: space-between;
}
.details>view:nth-child(1) {
  /* margin-right: 80rpx; */
}

.time image {
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
}

.time {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 300rpx;
}

.grade image {
  width: 30rpx;
  height: 25rpx;
  margin-right: 10rpx;
}

.grade {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  display: flex;
  align-items: center;
}
.location11{
