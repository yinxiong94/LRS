.Winningdetails {
  margin-top: 50rpx;
  display: flex;
  justify-content: space-around;
  width: 700rpx;
  height: 350rpx;
}

.circle {
  width: 124rpx;
  height: 124rpx;
  border-radius: 50%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  /* line-height: 36rpx; */
  margin-left: 49rpx;
  z-index: 10;
}

.circle1 {
  background: linear-gradient(0deg, rgba(95, 109, 232, 1), rgba(113, 183, 234, 1));
}

.circle2 {
  background: linear-gradient(180deg, rgba(84, 213, 160, 1), rgba(63, 190, 180, 1));
}

.circle3 {
  background: linear-gradient(0deg, rgba(251, 89, 126, 1), rgba(246, 190, 113, 1));
}

.circle::after {
  position: absolute;
  left: -13rpx;
  top: -14rpx;
  content: "";
  width: 150rpx;
  height: 150rpx;
  border: 1px dashed rgba(0, 160, 233, 0.4);
  border-radius: 50%;
  z-index: 1;
}

.Recorddetails {
  position: relative;
}

.score1 {
  box-shadow: 0rpx 0rpx 50rpx 5rpx #fed517 inset;
}

.score2 {
  box-shadow: 0rpx 0rpx 50rpx 5rpx #ffa42e inset;
}

.score {
  width: 217rpx;
  height: 242rpx;
  border-radius: 20rpx;
  background-color: #fff;
  position: absolute;
  z-index: 0;
  top: 68rpx;
  left: 0rpx;
}

.score_text {
  margin-top: 80rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 15rpx;
}

.shenglv text:nth-child(1) {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 36rpx;
  display: inline-block;
  margin-right: 15rpx;
}

.shenglv text:nth-child(2) {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(255, 164, 46, 1);
  line-height: 36rpx;
}

.jusheng {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 36rpx;
}

.jusheng text:nth-child(1) {
  display: inline-block;
  margin-right: 40rpx;
}

.dianshu {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 36rpx;
}

.image_list {
  margin-top: 30rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.list_data {
  position: relative;
}

.list_data>image {
  width: 220rpx;
  height: 220rpx;
  /* background:rgba(254,213,23,1); */
  border-radius: 10rpx;
  /* position: relative; */
}

.mvp {
  width: 80rpx;
  height: 40rpx;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 10rpx 0px 20rpx 0px;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 164, 46, 1);
  line-height: 40rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.cs {
  width: 220rpx;
  height: 50rpx;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 0px 0px 10rpx 10rpx;
  position: absolute;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.cs text {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.text text:nth-child(1) {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin-right: 20rpx;
}

.text text:nth-child(2) {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 164, 46, 1);
}

.image_list{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  text-align: center;
}

.image_list1{
  margin-bottom: 25rpx;
}