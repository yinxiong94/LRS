.container{
  padding: 30rpx;
}

.select_title {
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  width: 515rpx;
  display: flex;
  justify-content: space-between;
}

.select_title view {
  height: 60rpx;
}

.select_title_click {
  border-bottom: 6rpx solid rgba(254, 213, 23, 1);
  border-radius: 5rpx;
  color: rgba(51, 51, 51, 1);
  font-weight: bold;
}

.select_but {
  margin-top: 30rpx;
  width: 550rpx;
  display: flex;
  justify-content: space-between;
}

.select_but view {
  width: 150rpx;
  height: 50rpx;
  background: rgba(238, 238, 238, 1);
  border-radius: 30rpx;
  color: rgba(153, 153, 153, 1);
  font-size:26rpx;
  text-align: center;
  line-height: 46rpx;
}

.select_but1 {
  color: rgba(51, 51, 51, 1) !important;
  background-color: rgba(254, 213, 23, 1) !important;
}






