.container {
  padding: 30rpx;
}

.select_title {
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  width: 400rpx;
  display: flex;
  justify-content: space-between;
}

.select_title view {
  height: 60rpx;
}

.select_title_click {
  border-bottom: 6rpx solid rgba(254, 213, 23, 1);
  border-radius: 5rpx;
  color: rgba(51, 51, 51, 1);
  font-weight: bold;
}

.select_but {
  margin-top: 30rpx;
  width: 320rpx;
  display: flex;
  justify-content: space-between;
}

.select_but view {
  width: 140rpx;
  height: 46rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 10rpx;
  color: rgba(131, 131, 131, 1);
  font-size:22rpx;
  text-align: center;
  line-height: 46rpx;
}

.select_but1{
  color: #fff !important;
  background-color: rgba(255, 164, 46, 1) !important;
}
